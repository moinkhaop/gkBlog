{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["NEXT_PUBLIC_VERCEL_ENV", "NEXT_PUBLIC_VERCEL_URL"],
  "tasks": {
    "build": {
      "dependsOn": ["db:generate", "^build"],
      "outputs": ["dist/**", ".next/**"],
      "env": [
        "DATABASE_URL",
        "SALT_IP_ADDRESS"
      ]
    },
    "clean": {
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "dependsOn": ["db:generate"]
    },
    "lint": {
      "outputs": []
    },
    "start": {
      "cache": false,
      "dependsOn": ["^build"]
    }
  }
}
